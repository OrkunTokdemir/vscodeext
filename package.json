{
  "name": "qt",
  "displayName": "Qt",
  "description": "Qt Official Support",
  "version": "0.0.1",
  "homepage": "https://www.qt.io/",
  "icon": "res/icons/qt.png",
  "publisher": "theqtcompany",
  "repository": {
    "type": "git",
    "url": "https://codereview.qt-project.org/qt-labs/vscodeext"
  },
  "bugs": {
    "url": "https://bugreports.qt.io/projects/VSCODEEXT"
  },
  "engines": {
    "vscode": "^1.78.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "workspaceContains:*.pro",
    "workspaceContains:*.qrc",
    "workspaceContains:*.qdoc",
    "onCommand:cmake.activeFolderName",
    "onCommand:cmake.activeFolderPath",
    "onCommand:cmake.activeConfigurePresetName",
    "onCommand:cmake.activeBuildPresetName",
    "onCommand:cmake.activeTestPresetName",
    "onCommand:cmake.buildTargetName",
    "onCommand:cmake.launchTargetPath",
    "onCommand:cmake.launchTargetDirectory",
    "onCommand:cmake.launchTargetFilename",
    "onCommand:cmake.getLaunchTargetPath",
    "onCommand:cmake.getLaunchTargetDirectory",
    "onCommand:cmake.getLaunchTargetFilename",
    "onCommand:cmake.buildType",
    "onCommand:cmake.buildDirectory",
    "onCommand:cmake.executableTargets",
    "onCommand:cmake.buildKit",
    "onCommand:cmake.tasksBuildCommand",
    "onDebugResolve:cmake",
    "onDebugInitialConfigurations",
    "onDebugDynamicConfigurations:cmake",
    "workspaceContains:CMakeLists.txt",
    "workspaceContains:*/CMakeLists.txt",
    "workspaceContains:*/*/CMakeLists.txt",
    "workspaceContains:*/*/*/CMakeLists.txt",
    "workspaceContains:.vscode/cmake-kits.json"
  ],
  "main": "./out/extension.js",
  "l10n": "./l10n",
  "contributes": {
    "commands": [
      {
        "command": "vscode-qt-tools.registerQt",
        "title": "%command.vscode-qt-tools.registerQt.title%"
      },
      {
        "command": "vscode-qt-tools.selectQtPath",
        "title": "Qt: Pick Default Qt Installation"
      },
      {
        "command": "vscode-qt-tools.loadAndBuildQtProject",
        "title": "Qt: Load and Build Qt Project"
      },
      {
        "command": "vscode-qt-tools.openUiFileInQtDesigner",
        "title": "%command.qt.ui.openUiFileInQtDesigner.title%",
        "category": "UI",
        "icon": {
          "dark": "res/icons/qt.svg",
          "light": "res/icons/qt.svg"
        }
      }
    ],
    "languages": [
      {
        "id": "pro",
        "extensions": [
          ".pro"
        ],
        "aliases": [
          "Pro"
        ]
      },
      {
        "id": "qrc",
        "extensions": [
          ".qrc",
          ".qrc.cmakein"
        ],
        "aliases": [
          "Qrc"
        ]
      },
      {
        "id": "qdoc",
        "configuration": "./res/lang/qdoc/language-configuration.json",
        "extensions": [
          ".qdoc",
          ".qdocconf",
          ".qdocinc"
        ],
        "aliases": [
          "Qdoc",
          "QDoc",
          "qdoc"
        ]
      },
      {
        "id": "ui",
        "extensions": [
          ".ui"
        ],
        "aliases": [
          "Ui"
        ],
        "icon": {
          "light": "res/icons/qt.svg",
          "dark": "res/icons/qt.svg"
        }
      }
    ],
    "localizations": [],
    "menus": {
      "view/item/context": [
        {
          "command": "vscode-qt-tools.openUiFileInQtDesigner",
          "when": "resourceLangId == ui"
        }
      ],
      "editor/title/context": [
        {
          "command": "vscode-qt-tools.openUiFileInQtDesigner",
          "when": "resourceLangId == ui"
        }
      ],
      "editor/title": [
        {
          "command": "vscode-qt-tools.openUiFileInQtDesigner",
          "when": "resourceLangId == ui"
        }
      ],
      "explorer/context": [
        {
          "command": "vscode-qt-tools.openUiFileInQtDesigner",
          "when": "resourceLangId == ui"
        }
      ]
    },
    "grammars": [
      {
        "language": "qdoc",
        "scopeName": "source.qdoc",
        "path": "./res/lang/qdoc/qdoc.tmLanguage.json"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "Qt Configuration",
      "properties": {
        "vscode-qt-tools.qtFolders": {
          "type": "array",
          "default": [],
          "description": "Specify the Qt folder(s) if it is not found automatically.",
          "scope": "machine-overridable"
        },
        "vscode-qt-tools.qtInstallations": {
          "type": "array",
          "default": [],
          "description": "List of registered Qt installations.",
          "scope": "machine-overridable"
        },
        "vscode-qt-tools.selectedQtPath": {
          "type": "string",
          "default": "",
          "description": "Specify the default Qt installation.",
          "scope": "machine-overridable"
        }
      }
    }
  },
  "extensionDependencies": [
    "ms-vscode.cmake-tools"
  ],
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "npm run prettier && npx eslint . --fix",
    "gitgui": "git gc && git citool",
    "update": "( git pull --rebase --autostash origin dev ) || npm run gitgui",
    "unitTests": "npm run pretest && node ./out/test/unit/runTest.js",
    "integrationTests": "npm run pretest && node ./out/test/integration/runTest.js",
    "allTests": "ts-node ./src/scripts/run_all_tests.ts",
    "rebase": "git fetch && git -c sequence.editor=\"code --wait\" rebase -i --autostash origin/dev",
    "review": "npm run rebase && npm i && npm run pretest && npm run test && npm run gitgui && git push origin HEAD:refs/for/dev",
    "prettier": "prettier --write \"**/*.{js,ts,json}\"",
    "package": "vsce package --out out"
  },
  "devDependencies": {
    "@types/chai": "^4.3.10",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.2.5",
    "@types/vscode": "^1.78.0",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "@vscode/l10n-dev": "^0.0.30",
    "@vscode/test-electron": "^2.3.2",
    "@vscode/vsce": "^2.21.1",
    "chai": "^4.3.10",
    "commander": "^11.1.0",
    "eslint": "^8.50.0",
    "glob": "^10.3.9",
    "mocha": "^10.2.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2",
    "ts-node": "^10.4.0"
  },
  "dependencies": {
    "@vscode/l10n": "^0.0.16",
    "typescript": "^5.2.2",
    "ts-sinon": "^2.0.2"
  }
}
