{
  "name": "ci-scripts",
  "version": "0.9.0",
  "main": "out/index.js",
  "types": "out/index.d.ts",
  "files": [
    "out"
  ],
  "scripts": {
    "pretest:qt-core": "cd qt-core && npm run pretest",
    "pretest:qt-official": "cd qt-official && npm run pretest",
    "pretest:qt-ui": "cd qt-ui && npm run pretest",
    "pretest:qt-lib": "cd qt-lib && npm run pretest",
    "pretest:all": "npm run pretest:qt-lib && npm run pretest:qt-core && npm run pretest:qt-official && npm run pretest:qt-ui",
    "ci-lint:qt-core": "ts-node scripts/ci_lint.ts --dir=\"qt-core\" --exclude_licenses=\"qt-core\"",
    "ci-lint:qt-official": "ts-node scripts/ci_lint.ts --dir=\"qt-official\" --exclude_licenses=\"qt-official\"",
    "ci-lint:qt-ui": "ts-node scripts/ci_lint.ts --dir=\"qt-ui\" --exclude_licenses=\"qt-ui\"",
    "ci-lint:qt-lib": "npm run checkStyle -- --dir=\"qt-lib\" && npm run eslint -- --dir=\"qt-lib\" && npm run checkPackage -- --dir=\"qt-lib\"",
    "ci-lint:all": "npm run ci-lint && npm run ci-lint:qt-core && npm run ci-lint:qt-official && npm run ci-lint:qt-ui && npm run ci-lint:qt-lib",
    "ci-lint": "npm run prettierCheck && eslint .",
    "install-ext:qt-core": "ts-node scripts/install-ext.ts --dir=\"qt-core/out\" --name=\"qt-core\"",
    "install-ext:qt-official": "ts-node scripts/install-ext.ts --dir=\"qt-official/out\" --name=\"qt-official\"",
    "install-ext:qt-ui": "ts-node scripts/install-ext.ts --dir=\"qt-ui/out\" --name=\"qt-ui\"",
    "install-ext:all": "npm run install-ext:qt-core && npm run install-ext:qt-official && npm run install-ext:qt-ui",
    "install:qt-core": "cd qt-core && npm install",
    "install:qt-official": "cd qt-official && npm install",
    "install:qt-ui": "cd qt-ui && npm install",
    "install:qt-lib": "cd qt-lib && npm install",
    "install:all": "npm install && npm run install:qt-core && npm run install:qt-official && npm run install:qt-ui && npm run install:qt-lib",
    "eslint": "ts-node scripts/eslint.ts",
    "ci:qt-core": "cd qt-core && npm ci",
    "ci:qt-official": "cd qt-official && npm ci",
    "ci:qt-ui": "cd qt-ui && npm ci",
    "ci:qt-lib": "cd qt-lib && npm ci",
    "ci:all": "npm ci && npm run ci:qt-core && npm run ci:qt-official && npm run ci:qt-ui",
    "package:qt-core": "cd qt-core && npm run package",
    "package:qt-official": "cd qt-official && npm run package",
    "package:qt-ui": "cd qt-ui && npm run package",
    "package:all": "npm run package:qt-core && npm run package:qt-official && npm run package:qt-ui",
    "compile:qt-core": "cd qt-core && npm run compile",
    "compile:qt-official": "cd qt-official && npm run compile",
    "compile:qt-ui": "cd qt-ui && npm run compile",
    "compile:qt-lib": "cd qt-lib && npm run compile",
    "compile:all": "npm run compile && npm run compile:qt-core && npm run compile:qt-official && npm run compile:qt-ui && npm run compile:qt-lib",
    "checkLicenses": "ts-node scripts/check_licenses.ts",
    "checkPackage": "ts-node scripts/check_package.ts",
    "checkStyle": "ts-node scripts/check_style.ts",
    "generateLicenses": "ts-node scripts/generate_licenses.ts",
    "generateLicenses:qt-core": "npm run generateLicenses -- --dir=\"qt-core\" --exclude=\"qt-core\" --output=\"qt-core/ThirdPartyNotices.txt\"",
    "generateLicenses:qt-official": "npm run generateLicenses -- --dir=\"qt-official\" --exclude=\"qt-official\" --output=\"qt-official/ThirdPartyNotices.txt\"",
    "generateLicenses:qt-ui": "npm run generateLicenses -- --dir=\"qt-ui\" --exclude=\"qt-ui\" --output=\"qt-ui/ThirdPartyNotices.txt\"",
    "generateLicenses:all": "npm run generateLicenses:qt-core && npm run generateLicenses:qt-official && npm run generateLicenses:qt-ui",
    "prettierWrite": "npm run prettierBase -- --write \"scripts/**/*.{js,ts,json,mjs,cjs}\" --log-level silent",
    "prettierCheck": "npm run prettierBase -- --check \"scripts/**/*.{js,ts,json,mjs,cjs}\"",
    "prettierBase": "prettier --config \"common/.prettierrc\" --ignore-path \"common/.prettierignore\""
  },
  "devDependencies": {
    "@types/node": "^20.2.5",
    "@types/vscode": "^1.78.0",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "eslint": "^8.50.0",
    "glob": "^10.3.9",
    "license-checker": "^25.0.1",
    "prettier": "^3.0.3",
    "ts-node": "^10.4.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "commander": "^12.1.0",
    "typescript": "^5.2.2"
  }
}
